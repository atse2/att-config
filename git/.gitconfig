[user]
    email = atse@savioke.com
    name = Allison Tse

[credential]
    helper = cache --timeout=7200

[alias]
  co = checkout
  ci = commit
  st = status
  b = branch -a
  c = clone
  hist = log --graph --oneline --decorate --color
  type = cat-file -t
  dump = cat-file -p
  diffs = diff --staged
  r = remote -v

[color]
    ui = true
    [diff]
        whitespace = "red reverse"

[interactive]
  singlekey = true
